package com.qa.demo;

public class Conditionals {

	public static void main(String[] args) {

		int num = 12;

		if (num < 10 || num % 12 == 1) {
			System.out.println("Condition met!");
		} else if(num == 12) {
			System.out.println("num is 12");
		} else {
			System.out.println("No conditions met");
		}

	}

}
